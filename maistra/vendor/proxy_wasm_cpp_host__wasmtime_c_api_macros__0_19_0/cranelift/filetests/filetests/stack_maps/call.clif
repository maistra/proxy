test stack_maps
set enable_safepoints=true
target x86_64 legacy

function %icall_fast(r64) -> r64 fast {
; check:  function %icall_fast
; nextln:     ss0 = spill_slot 8, offset -32
    fn0 = %none()
block0(v0: r64):
; check: ss0]         v0 = spill v2
; check:              safepoint v0
    call fn0()
    return v0
}
; check:  Stack maps:
; nextln: 
; nextln: safepoint v0
; nextln:   - mapped words: 4
; nextln:   - live: [0]

