test compile
set unwind_info=false
target aarch64

function %uaddsat64(i64, i64) -> i64 {
block0(v0: i64, v1: i64):
  v2 = uadd_sat.i64 v0, v1
  return v2
}

; check: stp fp, lr, [sp, #-16]!
