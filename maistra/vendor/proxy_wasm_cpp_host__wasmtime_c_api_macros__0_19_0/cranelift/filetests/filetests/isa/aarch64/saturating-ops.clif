; nextln: uqadd d0, d0, d1
; nextln: mov x0, v0.d[0]
; nextln: ldp fp, lr, [sp], #16
; nextln: ret

function %uaddsat8(i8, i8) -> i8 {
block0(v0: i8, v1: i8):
  v2 = uadd_sat.i8 v0, v1
  return v2
}

