test compile
set enable_simd
target i686 legacy haswell

function u0:1(i32) -> i64 system_v {
    block1(v0: i32):
        v1 = load.i64 notrap aligned v0+0
        v2 = load.i32 notrap aligned v0+16
        v3 = ishl v1, v2
        return v3
}

