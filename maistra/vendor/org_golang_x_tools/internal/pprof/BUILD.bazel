load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "pprof",
    srcs = ["pprof.go"],
    importpath = "golang.org/x/tools/internal/pprof",
    visibility = ["//:__subpackages__"],
)

alias(
    name = "go_default_library",
    actual = ":pprof",
    visibility = ["//:__subpackages__"],
)

go_test(
    name = "pprof_test",
    srcs = ["pprof_test.go"],
    data = glob(["testdata/**"]),
    deps = [":pprof"],
)
