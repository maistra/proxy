-// hashDiagnostics computes a hash to identify diags.
-func hashDiagnostics(diags ...*source.Diagnostic) string {
-	source.SortDiagnostics(diags)
-	h := sha256.New()
-	for _, d := range diags {
-		for _, t := range d.Tags {
-			fmt.Fprintf(h, "%s", t)
-		}
-		for _, r := range d.Related {
