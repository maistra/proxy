-	for _, rule := range postfixTmpls {
-		// When completing foo.print<>, "print" is naturally overwritten,
-		// but we need to also remove "foo." so the snippet has a clean
-		// slate.
-		edits, err := c.editText(sel.Pos(), afterDot, "")
