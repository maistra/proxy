-				m.Module != nil &&
-				m.Module.Path != "" &&
-				m.Module.Version != "" &&
-				!source.IsWorkspaceModuleVersion(m.Module.Version) {
-				urlPath = strings.Replace(urlPath, m.Module.Path, m.Module.Path+"@"+m.Module.Version, 1)
