-
-	// Replace the portion (possibly empty) of the spec before the path:
-	//     local "path"      or      "path"
-	//   ->      <-                -><-
-	rng := span.NewRange(tokFile, spec.Pos(), spec.Path.Pos())
-	spn, err := rng.Span()
-	if err != nil {
-		return nil, err
-	}
-
-	return &diff.Edit{
-		Start: spn.Start().Offset(),
-		End:   spn.End().Offset(),
-		New:   newText,
-	}, nil
-}
