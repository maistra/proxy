-	// Don't expect fields that we can't populate from the command output.
-	for _, diag := range want {
-		if diag.Source == "no_diagnostics" {
-			continue // see DiffDiagnostics
-		}
-		diag.Source = ""
-		diag.Severity = 0
-	}
-
-	tests.CompareDiagnostics(t, uri, want, got)
