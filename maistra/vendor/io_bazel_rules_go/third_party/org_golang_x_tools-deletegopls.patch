-	// This corresponds to v2's UnmarshalStrict.
-	d.KnownFields(true)
-	var r Report
-	if err := d.Decode(&r); err != nil {
-		return nil, fmt.Errorf("yaml.Decode: %v", err)
-	}
-	return &r, nil
