-	var summary strings.Builder
-	for _, e := range expectations {
-		v := e.Check(s)
-		if v > finalVerdict {
-			finalVerdict = v
-		}
-		summary.WriteString(fmt.Sprintf("%v: %s\n", v, e.Description()))
