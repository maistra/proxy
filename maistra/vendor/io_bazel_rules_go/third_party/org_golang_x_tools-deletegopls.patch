-// precedingToken scans src to find the token preceding pos.
-func precedingToken(pos token.Pos, tok *token.File, src []byte) token.Token {
-	s := &scanner.Scanner{}
-	s.Init(tok, src, nil, 0)
-
-	var lastTok token.Token
-	for {
-		p, t, _ := s.Scan()
-		if t == token.EOF || p >= pos {
-			break
-		}
-
-		lastTok = t
-	}
-	return lastTok
-}
