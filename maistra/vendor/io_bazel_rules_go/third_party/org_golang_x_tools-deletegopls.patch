-		score := c.matcher.Score(rule.label)
-		if score <= 0 {
-			continue
-		}
-
-		c.items = append(c.items, CompletionItem{
-			Label:               rule.label + "!",
-			Detail:              rule.details,
-			Score:               float64(score) * 0.01,
-			Kind:                protocol.SnippetCompletion,
