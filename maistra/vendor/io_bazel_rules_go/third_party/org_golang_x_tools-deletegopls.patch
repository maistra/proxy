-// ipcChild is the portion of TestIPC that runs in a child process.
-func ipcChild() {
-	getenv := func(name string) (key [32]byte) {
-		s, _ := strconv.Unquote(os.Getenv(name))
-		copy(key[:], []byte(s))
-		return
-	}
-
-	// Verify key A.
-	got, err := filecache.Get(testIPCKind, getenv("KEYA"))
