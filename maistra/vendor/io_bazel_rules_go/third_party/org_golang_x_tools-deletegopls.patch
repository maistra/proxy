-func messageType(l log.Level) protocol.MessageType {
-	switch l {
-	case log.Error:
-		return protocol.Error
-	case log.Warning:
-		return protocol.Warning
-	case log.Debug:
-		return protocol.Log
-	}
-	return protocol.Info
-}
