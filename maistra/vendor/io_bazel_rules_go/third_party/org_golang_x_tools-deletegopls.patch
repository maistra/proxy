-// EnvSlice returns Env as a slice of k=v strings.
-func (u *UserOptions) EnvSlice() []string {
-	var result []string
-	for k, v := range u.Env {
-		result = append(result, fmt.Sprintf("%v=%v", k, v))
-	}
-	return result
-}
