-type gobDiagnostic struct {
-	Location       protocol.Location
-	Category       string
-	Message        string
-	SuggestedFixes []gobSuggestedFix
-	Related        []gobRelatedInformation
-}
-
-type gobRelatedInformation struct {
-	Location protocol.Location
-	Message  string
-}
-
-type gobSuggestedFix struct {
-	Message   string
-	TextEdits []gobTextEdit
-}
