-func (t *Tracker) Cancel(token protocol.ProgressToken) error {
-	t.mu.Lock()
-	defer t.mu.Unlock()
-	wd, ok := t.inProgress[token]
-	if !ok {
-		return fmt.Errorf("token %q not found in progress", token)
-	}
-	if wd.cancel == nil {
-		return fmt.Errorf("work %q is not cancellable", token)
-	}
-	wd.doCancel()
-	return nil
-}
