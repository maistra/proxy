-
-	cursor := c.surrounding.cursor
-	for _, field := range fields.List {
-		for _, name := range field.Names {
-			if name.String() == "_" {
-				continue
-			}
-			obj := c.pkg.GetTypesInfo().ObjectOf(name)
-			if obj == nil {
-				continue
-			}
