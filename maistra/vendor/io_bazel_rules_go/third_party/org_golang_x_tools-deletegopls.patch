-	case event.IsEnd(ev):
-		span, stats := r.getRPCSpan(ctx, ev)
-		if stats != nil {
-			endRPC(ctx, ev, span, stats)
-		}
-	case event.IsMetric(ev):
-		sent := byteUnits(tag.SentBytes.Get(lm))
-		rec := byteUnits(tag.ReceivedBytes.Get(lm))
-		if sent != 0 || rec != 0 {
-			if _, stats := r.getRPCSpan(ctx, ev); stats != nil {
