-// dirNonClean is filepath.Dir, without the Clean at the end.
-func dirNonClean(path string) string {
-	vol := filepath.VolumeName(path)
-	i := len(path) - 1
-	for i >= len(vol) && !os.IsPathSeparator(path[i]) {
-		i--
-	}
-	return path[len(vol) : i+1]
-}
