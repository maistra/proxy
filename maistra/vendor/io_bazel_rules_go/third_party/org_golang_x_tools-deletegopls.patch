-	// For a bit of type safety, use reflection to get the interface name within
-	// the package scope.
-	it := reflect.TypeOf((*command.Interface)(nil)).Elem()
-	obj := pkg.Types.Scope().Lookup(it.Name()).Type().Underlying().(*types.Interface)
