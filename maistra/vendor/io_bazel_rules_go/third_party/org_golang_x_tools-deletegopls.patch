-func generateIdentifier(idx int, prefix string, hasCollision func(string) bool) (string, int) {
-	name := prefix
-	if idx != 0 {
-		name += fmt.Sprintf("%d", idx)
-	}
-	for hasCollision(name) {
-		idx++
-		name = fmt.Sprintf("%v%d", prefix, idx)
-	}
-	return name, idx + 1
-}
