-type symbolStore struct {
-	res [maxSymbols]symbolInformation
-}
-
-// store inserts si into the sorted results, if si has a high enough score.
-func (sc *symbolStore) store(si symbolInformation) {
-	if sc.tooLow(si.score) {
-		return
-	}
