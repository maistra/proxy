-// RecvIdent returns the type identifier of a method receiver.
-// e.g. A for all of A, *A, A[T], *A[T], etc.
-func RecvIdent(recv *ast.FieldList) *ast.Ident {
-	if recv == nil || len(recv.List) == 0 {
-		return nil
-	}
-	x := recv.List[0].Type
-	if star, ok := x.(*ast.StarExpr); ok {
-		x = star.X
