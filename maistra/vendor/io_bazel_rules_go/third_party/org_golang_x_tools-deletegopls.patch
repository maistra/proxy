-func packageSymbolMatch(space []string, name string, pkg *Metadata, matcher matcherFunc) ([]string, float64) {
-	qualified := append(space, string(pkg.Name), ".", name)
-	if _, s := matcher(qualified); s > 0 {
-		return qualified, s
-	}
-	return nil, 0
-}
