-			verdict := e.Check(s)
-			if verdict == Met {
-				return Met
-			}
-		}
-		return Unmet
-	}
-	description := describeExpectations(anyOf...)
-	return &SimpleExpectation{
-		check:       check,
-		description: fmt.Sprintf("Any of:\n%s", description),
-	}
