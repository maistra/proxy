-		if i.rpcs != nil {
-			ctx = i.rpcs.ProcessEvent(ctx, ev, lm)
-		}
-		if i.traces != nil {
-			ctx = i.traces.ProcessEvent(ctx, ev, lm)
-		}
-		if event.IsLog(ev) {
-			if s := cache.KeyCreateSession.Get(ev); s != nil {
-				i.State.addClient(s)
-			}
-			if sid := tag.NewServer.Get(ev); sid != "" {
-				i.State.updateServer(&Server{
-					ID:           sid,
