-			b.WriteString(ident.Name)
-			b.WriteString(".")
-		}
-	}
-	b.WriteString(name)
-
-	rng, err := w.mapper.Range(node.Pos(), node.End())
-	if err != nil {
-		w.error(err)
-		return
-	}
-	sym := source.Symbol{
-		Name:  b.String(),
-		Kind:  kind,
-		Range: rng,
-	}
-	w.symbols = append(w.symbols, sym)
-}
