-	}
-
-	sort.Strings(completions)
-
-	var items []protocol.CompletionItem
-	for _, c := range completions {
-		items = append(items, protocol.CompletionItem{
-			Label:      c,
-			InsertText: c,
-		})
-	}
-	return &protocol.CompletionList{Items: items}, nil
-}
