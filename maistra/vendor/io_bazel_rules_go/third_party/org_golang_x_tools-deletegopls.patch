-	// newline thinking "bar" is part of our selector.
-	if startLine := tokFile.Line(sel.Pos()); startLine != tokFile.Line(afterDot) {
-		if tokFile.Line(c.pos) != startLine {
-			return
-		}
-		afterDot = c.pos
-	}
