-
-	// Verify keyB.
-	got, err := filecache.Get(testIPCKind, keyB)
-	if err != nil {
-		t.Fatal(err)
-	}
-	if string(got) != "world" {
-		t.Fatalf("Get(keyB) = %q, want %q", got, "world")
-	}
