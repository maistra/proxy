-// TODO(rfindley): rename this function.
-func hoverTypeSpec(spec *ast.TypeSpec, decl *ast.GenDecl) *HoverContext {
-	comment := spec.Doc
-	if comment == nil && decl != nil {
-		comment = decl.Doc
-	}
-	if comment == nil {
-		comment = spec.Comment
-	}
-	return &HoverContext{
-		signatureSource: spec,
-		Comment:         comment,
-	}
-}
