-func (s *snapshot) TypeCheck(ctx context.Context, mode source.TypecheckMode, ids ...PackageID) ([]source.Package, error) {
-	// Build all the handles...
-	var phs []*packageHandle
-	for _, id := range ids {
-		parseMode := source.ParseFull
