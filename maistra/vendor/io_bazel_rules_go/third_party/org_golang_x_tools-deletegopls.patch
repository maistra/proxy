-func typesCost(scope *types.Scope) int64 {
-	cost := 64 + int64(scope.Len())*128 // types.object looks pretty big
-	for i := 0; i < scope.NumChildren(); i++ {
-		cost += typesCost(scope.Child(i))
-	}
-	return cost
-}
