-func getPrePost(content []byte, offset int) (string, string) {
-	pre, post := string(content)[:offset], string(content)[offset:]
-	if i := strings.LastIndex(pre, "\n"); i >= 0 {
-		pre = pre[i+1:]
-	}
-	if i := strings.IndexRune(post, '\n'); i >= 0 {
-		post = post[:i]
-	}
-	return pre, post
-}
