-func astCost(f *ast.File) int64 {
-	if f == nil {
-		return 0
-	}
-	var count int64
-	ast.Inspect(f, func(_ ast.Node) bool {
-		count += 32 // nodes are pretty small.
-		return true
-	})
-	return count
-}
