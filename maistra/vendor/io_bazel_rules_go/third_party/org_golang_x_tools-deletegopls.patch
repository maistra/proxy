-			t.Fatalf("output line not of form 'span: message': %q", line)
-		}
-		spn, message := span.Parse(parts[0]), parts[1]
-		rng, err := mapper.Range(spn)
-		if err != nil {
-			t.Fatal(err)
-		}
-		// Set only the fields needed by DiffDiagnostics.
-		got = append(got, &source.Diagnostic{
-			URI:     uri,
-			Range:   rng,
-			Message: message,
-		})
-	}
