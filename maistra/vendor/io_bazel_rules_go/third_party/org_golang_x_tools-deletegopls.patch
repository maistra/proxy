-func (e Edit) toProtocolChangeEvent() protocol.TextDocumentContentChangeEvent {
-	return protocol.TextDocumentContentChangeEvent{
-		Range: &protocol.Range{
-			Start: e.Start.ToProtocolPosition(),
-			End:   e.End.ToProtocolPosition(),
-		},
-		Text: e.Text,
-	}
-}
