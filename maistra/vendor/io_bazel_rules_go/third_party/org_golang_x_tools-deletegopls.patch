-func (c *completer) packageMembers(pkg *types.Package, score float64, imp *importInfo, cb func(candidate)) {
-	scope := pkg.Scope()
-	for _, name := range scope.Names() {
-		obj := scope.Lookup(name)
-		cb(candidate{
-			obj:         obj,
-			score:       score,
-			imp:         imp,
-			addressable: isVar(obj),
-		})
-	}
-}
