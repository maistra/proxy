-	// so check here to avoid panicking.
-	if s.Start().Offset() > tf.Size() {
-		return Range{}, bug.Errorf("start offset %v is past the end of the file %v", s.Start(), tf.Size())
-	}
-	if s.End().Offset() > tf.Size() {
