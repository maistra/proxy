-	for i, obj := range cand.path {
-		buf.WriteString(obj.Name())
-		if cand.pathInvokeMask&(1<<uint16(i)) > 0 {
-			buf.WriteByte('(')
-			buf.WriteByte(')')
-		}
-		buf.WriteByte('.')
