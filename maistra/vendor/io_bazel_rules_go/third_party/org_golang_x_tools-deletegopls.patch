-		return err
-	}
-	err = r.encode(f)
-	err2 := f.Close()
-	if err == nil {
-		err = err2
-	}
-	return err
-}
-
-// ToString encodes r to a YAML string.
-func (r *Report) ToString() (string, error) {
-	var b strings.Builder
-	if err := r.encode(&b); err != nil {
-		return "", err
-	}
-	return b.String(), nil
-}
