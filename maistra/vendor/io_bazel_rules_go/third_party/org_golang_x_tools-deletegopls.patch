-		tmplArgs := postfixTmplArgs{
-			X:              source.FormatNode(c.pkg.FileSet(), sel.X),
-			StmtOK:         stmtOK,
-			Obj:            exprObj(c.pkg.GetTypesInfo(), sel.X),
-			Type:           selType,
