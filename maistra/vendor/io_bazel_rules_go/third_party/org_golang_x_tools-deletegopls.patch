-	switch ix := x.(type) { // check for instantiated receivers
-	case *ast.IndexExpr:
-		x = ix.X
-	case *typeparams.IndexListExpr:
-		x = ix.X
-	}
-	if ident, ok := x.(*ast.Ident); ok {
-		return ident
-	}
