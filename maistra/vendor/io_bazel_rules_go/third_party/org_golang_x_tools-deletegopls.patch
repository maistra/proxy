-func sendError(ctx context.Context, reply jsonrpc2.Replier, err error) {
-	err = fmt.Errorf("%v: %w", err, jsonrpc2.ErrParse)
-	if err := reply(ctx, nil, err); err != nil {
-		event.Error(ctx, "", err)
-	}
