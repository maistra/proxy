-	// Check cache again in case another thread got there first.
-	if prev, ok := s.packages.Get(packageKey); ok {
-		prevPH := prev.(*packageHandle)
-		release()
-		if prevPH.m != ph.m {
-			return nil, bug.Errorf("existing package handle does not match for %s", ph.m.ID)
-		}
-		return prevPH, nil
-	}
