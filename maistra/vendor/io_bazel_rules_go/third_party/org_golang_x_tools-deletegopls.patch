-	t.mu.Unlock()
-	wd.cleanup = func() {
-		t.mu.Lock()
-		delete(t.inProgress, token)
-		t.mu.Unlock()
-	}
-	err := wd.client.Progress(ctx, &protocol.ProgressParams{
-		Token: wd.token,
-		Value: &protocol.WorkDoneProgressBegin{
-			Kind:        "begin",
-			Cancellable: wd.cancel != nil,
-			Message:     message,
-			Title:       title,
-		},
