-func fromProtocolTextEdit(textEdit protocol.TextEdit) Edit {
-	return Edit{
-		Start: fromProtocolPosition(textEdit.Range.Start),
-		End:   fromProtocolPosition(textEdit.Range.End),
-		Text:  textEdit.NewText,
-	}
-}
