-func (l *fieldLoader) loadField(pkg *packages.Package, obj *types.Var, doc, tag string) (*Field, error) {
-	if existing, ok := l.loaded[obj]; ok {
-		return existing, nil
-	}
-	fld := &Field{
-		Name:    obj.Name(),
-		Doc:     strings.TrimSpace(doc),
-		Type:    obj.Type(),
-		JSONTag: reflect.StructTag(tag).Get("json"),
