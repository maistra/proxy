-					addPkg(imp)
-				}
-			}
-		}
-		addPkg(searchpkg)
-		for _, obj := range objs {
-			if obj.Parent() == types.Universe {
-				return nil, fmt.Errorf("%q: %w", obj.Name(), errBuiltin)
-			}
-			pkg, ok := pkgs[obj.Pkg()]
-			if !ok {
