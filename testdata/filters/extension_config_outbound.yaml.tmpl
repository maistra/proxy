- name: mx_outbound{{.N}}
  config_discovery:
    config_source:
      api_config_source:
        api_type: GRPC
        transport_api_version: V3
        grpc_services:
        - envoy_grpc:
            cluster_name: xds_cluster
    type_urls:
    - envoy.extensions.filters.http.wasm.v3.Wasm
- name: stats_outbound{{.N}}
  config_discovery:
    config_source:
      api_config_source:
        api_type: GRPC
        transport_api_version: V3
        grpc_services:
        - envoy_grpc:
            cluster_name: xds_cluster
    type_urls:
    - envoy.extensions.filters.http.wasm.v3.Wasm
    - stats.PluginConfig
